<% Show.twitter({ :city => "stockholm" }).results.map do |tweet| %>
  <div class="twitter">
    <div class="media">
      <a class="pull-left" href="#">
       <img class="media-object" data-src="holder.js/64x64">
      </a>
      <div class="media-body">
        <h4 class="media-heading"><%= tweet.text %></h4>
        <div class="media">
          <b><%= tweet.created_at %></b>
          <blockquote>
            <%= tweet.user.name %></h2>
        </blockquote>
      </div>
    </div>
  </div>
</div>
  <% end %>
